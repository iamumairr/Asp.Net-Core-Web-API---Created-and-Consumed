{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.0",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.min.map",
        "jquery.js"
      ]
    },
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/lib/jqueryui/"
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "library": "limonte-sweetalert2@11.1.4",
      "destination": "wwwroot/lib/limonte-sweetalert2/",
      "files": [
        "sweetalert2.min.js",
        "sweetalert2.min.css"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "sprites/brands.svg",
        "sprites/regular.svg",
        "sprites/solid.svg",
        "webfonts/fa-brands-400.eot",
        "webfonts/fa-brands-400.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-regular-400.eot",
        "webfonts/fa-regular-400.svg",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-solid-900.eot",
        "webfonts/fa-solid-900.svg",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "js/all.min.js"
      ]
    }
  ]
}