@{
    ViewData["Title"] = "Home - National Parks";
}
<br />
<div class="border p-3 bg-white">
    <div class="row">
        <div class="col-6">
            <h2 class="text-info">National Parks List</h2>
        </div>
        <div class="col-6">
            <a class="btn btn-outline-success float-end" asp-action="Upsert"><i class="fas fa-plus"></i> Create New National Park</a>
        </div>
    </div>
    <br /><br />
    <table id="tableData" class="table table-striped table-bordered w-100">
        <thead>
            <tr>
                <th>Name</th>
                <th>State</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/nationalpark.js"></script>
}